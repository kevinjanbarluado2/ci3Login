RewriteEngine on
RewriteCond $1 !^(index\.php|assets|js|resource|robots\.txt)
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php/$1 [L,QSA]

# RewriteCond %{HTTPS} off
# RewriteCond %{HTTP_HOST} ^www.onlineinsure.co.nz [NC]
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://onlineinsure.co.nz/compliance/$1 [L,R=301]

RewriteCond %{HTTP_HOST} ^www\. [NC]
RewriteRule ^(.*)$ https://onlineinsure.co.nz/compliance/$1 [R=301,L]

php_value max_input_vars 10000
